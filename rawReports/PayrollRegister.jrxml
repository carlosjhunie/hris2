	<parameter name="payrollPeriodId" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select distinct epr.*, e.empNo, e.lastname + ', ' + e.firstname as formattedName, pp.fromDate, pp.toDate, pp.payrollCode,
et. employeeTypeName
from payrollPeriod pp, employeePayrollRun epr, employee e, employeeType et
where e.empId = epr.empId
and et.employeeTypeId = e.employeeTypeId
and pp.payrollPeriodId = epr.payrollPeriodId
and epr.payrollPeriodId =  $P{payrollPeriodId} 
order by formattedName, employeeTypeName]]>
	</queryString>
